<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kosmo.broadcastreceiver38_3_sender">
    <!--방송 수신 앱이 종료된 상태에서도 현재 앱(송신 앱)이 보낸 방송을 받을 수 있도록
      사용자 정의 퍼미션추가-->
    <permission android:name="com.kosmo.broadcastreceiver.MY_PERMISSION" android:protectionLevel="signature"/>
    <uses-permission android:name="com.kosmo.broadcastreceiver.MY_PERMISSION"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BroadcastReceiver38_3_sender">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>