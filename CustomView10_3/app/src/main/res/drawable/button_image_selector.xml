<?xml version="1.0" encoding="utf-8"?>
<!-- 같은 속성(state_pressed) 중복시
      먼저 기술한 속성이 우선되므로 순서에 주의해야함.
     디폴트 이미지는 항상 맨 마지막에 둘 것.
     pressed가 enabled보다 먼저 작성되어야 한다.
-->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@drawable/press1_button" android:state_pressed="true"/>
    <item android:drawable="@drawable/press2_button" android:state_pressed="true"/>
    <!--
            <item android:drawable="@drawable/enabled_button" android:state_enabled="true" />
    -->
    <item android:drawable="@drawable/default_button"/>

</selector>